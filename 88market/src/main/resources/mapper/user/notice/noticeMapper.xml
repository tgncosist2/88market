<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.user.DAO.NoticeDAO">
    <select id="selectNoticeList" resultType="kr.co.sist.DTO.NoticeDTO">
        SELECT * FROM NOTICE WHERE STATUS_TYPE = '공지' ORDER BY INPUT_DATE DESC
    </select>
</mapper>
