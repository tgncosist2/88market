<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.user.DAO.LoginDAO">
    <select id="selectLoginList" resultType="kr.co.sist.DTO.UserDTO" parameterType="String">
        SELECT USER_NUM,EMAIL,PASS
        FROM USERS
        WHERE EMAIL=#{email}
    </select>
</mapper>
