<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.sist.filter.CustomUserMapper">
    <select id="selectUserByUserNum" resultType="kr.co.sist.DTO.UserDTO">
        SELECT USER_NUM FROM USERS WHERE USER_NUM = #{userNum}
    </select>
</mapper>
